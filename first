import java.util.Scanner;
public class Exercise {
	public static void main(String[] args) {
		
		int input;
		System.out.println("숫자 하나를 입력하세요.");
		Scanner scanner = new Scanner(System.in);
		String tmp = scanner.nextLine();
		input = Integer.parseInt(tmp);
		
		if(input==0) System.out.println("입력하신 숫자는 0입니다");
		else System.out.printf("입력하신 숫자는 %d입니다.%n%n", input);

  	int score = 0;
		char grade = ' ';
		System.out.println("성적을 입력하세요");
    Scanner scanner2 = new Scanner(System.in);
		String tmp2 = scanner2.nextLine();
		score = Integer.parseInt(tmp2);
  	if(score>=90) {grade='A'; System.out.println(grade);}
		else if(score>=80) {grade='B'; System.out.println(grade);}
		else if(score>=70) {grade='C'; System.out.println(grade);}
		else if(score>=60) {grade='D'; System.out.println(grade);}
		else   {grade='F'; System.out.println(grade);}
